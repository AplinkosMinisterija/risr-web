{
  "name": "risr-web",
  "version": "0.1.0",
  "scripts": {
    "start": "vite --port 8080",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:sarif": "set SARIF_ESLINT_EMBED=true && yarn run lint --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "prepare": "husky install"
  },

  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.6.8",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-paginate": "^8.1.3",
    "react-query": "^3.39.3",
    "react-redux": "^7.2.8",
    "react-resize-detector": "^7.1.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.7",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@aplinkosministerija/biip-prettier-config": "^1.1.0",
    "@aplinkosministerija/eslint-config-biip-react": "^1.0.0",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/react-datepicker": "^4.4.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^8.0.3",
    "prettier": "^2.6.2",
    "vite": "^5.2.6",
    "vite-plugin-pwa": "^0.19.7"
  },
  "eslintConfig": {
    "extends": "@aplinkosministerija/eslint-config-biip-react"
  },
  "prettier": "@aplinkosministerija/biip-prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,html,css}": "prettier --write"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  }
}
